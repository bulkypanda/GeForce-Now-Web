{
   "name": "GeForce Now Web",
   "version": "1.0",
   "description": "Enable NVIDIA GeForce Now in the browser on unsupported devices!",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "permissions": ["webRequest", "webRequestBlocking", "https://play.geforcenow.com/*"],
   "background": {
      "scripts": ["background.js"],
      "persistent": true
   },
   "browser_action": {
      "default_icon": {
         "16": "images/16.png",
         "48": "images/48.png",
         "64": "images/64.png",
         "128": "images/128.png"
      },
      "default_popup": "popup.html",
      "default_title": "GeForce Now Web"
   },
   "icons": {
      "16": "images/16.png",
      "48": "images/48.png",
      "64": "images/64.png",
      "128": "images/128.png"
   },
   "manifest_version": 2
}

